{%- assign id = section.id -%}

{% comment %} Content settings {% endcomment %}
{%- assign blocks = section.blocks -%}
{% comment %} End content settings {% endcomment %}
{%- assign css_class = section.settings.css_class -%}
<style>

</style>
<!--  -->
<section class="section" id="" data-aos="" data-aos-duration="">
  <div class="clearfix frontpage_product_stagger--false">
    <h1 class="text-align-center">{{section.settings.heading}}</h1>
    <!--Start tab labels--> 
    <div class="tab-heading">
      <ul class="tabs heading-line">
        {% if blocks.size > 0 %}
        {% for block in blocks %}
        
        {% if forloop.first == true and block.settings.title != blank %}
        
        <li> <a class="active" href="#{{block.settings.title| remove: " " | append : "-"}}" data-no-instant="true">{{block.settings.title}}</a></li>
        
        {% elsif block.settings.title != blank %}
        
        <li> <a class="" href="#{{block.settings.title| remove: " " | append : "-"}}" data-no-instant="true">{{block.settings.title}}</a></li>
        
        {% endif %}

   
        {% endfor %}
        {% endif %}           
      </ul>
      <!--Start tab content--> 
      <ul class="tabs-content">
<!--       {% if blocks.size > 0 %} -->
        {% for block in blocks %}
        
        {% if forloop.first == true and block.settings.title != blank %}
<!--     active element     -->
        <li class="active" id="{{block.settings.title| remove: " " | append : "-"}}" style="display: block;">
          <div class="feature-wrapper">
            <div class="left-features-list mobile_hidden">
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="200" data-handle="durable">
                <div class="feature-img"> {{ block.settings.image_1| img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                    <h2>{{block.settings.title_1}}</h2>
                    <p>{{block.settings.subtitle_1}}</p>
                  </div>
                </div>
              </div>
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="400" data-handle="low-maintenance">
                <div class="feature-img"> {{ block.settings.image_2 | img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                    <h2>{{block.settings.title_2}}</h2>
                    <p>{{block.settings.subtitle_2}}</p>
                  </div>
                </div>
              </div>
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="600" data-handle="rodent-proof">
                <div class="feature-img"> {{ block.settings.image_3| img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                    <h2>{{block.settings.title_3}}</h2>
                    <p>{{block.settings.subtitle_3}}</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="middle-features-list">
              <div class="featued-img-wrapper"> {{ block.settings.image_middle | img_tag }}</div>
            </div>
            <div class="right-features-list mobile_hidden">
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="200" data-handle="modular">
                <div class="feature-img"> {{ block.settings.image_1l  | img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                  <h2>  {{block.settings.title_1l}}</h2>
                    <p>{{block.settings.subtitle_1l}}</p>
                  </div>
                </div>
              </div>
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="400" data-handle="lockable">
                <div class="feature-img"> {{ block.settings.image_2l  | img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                  <h2>  {{block.settings.title_2l}}</h2>
                    <p>{{block.settings.subtitle_2l}}</p>
                  </div>
                </div>
              </div>
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="600" data-handle="planters">
                <div class="feature-img"> {{ block.settings.image_3l  | img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                   <h2> {{block.settings.title_3l}}</h2>
                    <p>{{block.settings.subtitle_3l}}</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </li>
        
        
        {% else block.settings.title != blank %}
        
<!--     de-active element     -->
           <li class="" id="{{block.settings.title| remove: " " | append : "-"}}" style="display: none;">
          <div class="feature-wrapper">
            <div class="left-features-list mobile_hidden">
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="200" data-handle="durable">
                <div class="feature-img"> {{ block.settings.image_1| img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                    <h2>{{block.settings.title_1}}</h2>
                    <p>{{block.settings.subtitle_1}}</p>
                  </div>
                </div>
              </div>
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="400" data-handle="low-maintenance">
                <div class="feature-img"> {{ block.settings.image_2 | img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                    <h2>{{block.settings.title_2}}</h2>
                    <p>{{block.settings.subtitle_2}}</p>
                  </div>
                </div>
              </div>
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="600" data-handle="rodent-proof">
                <div class="feature-img"> {{ block.settings.image_3| img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                    <h2>{{block.settings.title_3}}</h2>
                    <p>{{block.settings.subtitle_3}}</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="middle-features-list">
              <div class="featued-img-wrapper"> {{ block.settings.image_middle | img_tag }}</div>
            </div>
            <div class="right-features-list mobile_hidden">
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="200" data-handle="modular">
                <div class="feature-img"> {{ block.settings.image_1l  | img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                  <h2>  {{block.settings.title_1l}}</h2>
                    <p>{{block.settings.subtitle_1l}}</p>
                  </div>
                </div>
              </div>
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="400" data-handle="lockable">
                <div class="feature-img"> {{ block.settings.image_2l  | img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                  <h2>  {{block.settings.title_2l}}</h2>
                    <p>{{block.settings.subtitle_2l}}</p>
                  </div>
                </div>
              </div>
              <div class="prod-feature aos-init aos-animate" data-aos="fade-up" data-aos-duration="800" data-aos-delay="600" data-handle="planters">
                <div class="feature-img"> {{ block.settings.image_3l  | img_tag }}</div>
                <div class="featuer-contents">
                  <div class="feature-title">
                   <h2> {{block.settings.title_3l}}</h2>
                    <p>{{block.settings.subtitle_3l}}</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </li>
        
        {% endif %}

        {% endfor %}
<!--         {% endif %}    -->
      
      
        
     
      </ul>
    </div>
  </div>
</section>
<!--  -->

